.table > thead > tr > th, .table > tbody > tr > th,
 .table > tfoot > tr > th, .table > thead > tr > td, 
 .table > tbody > tr > td, .table > tfoot > tr > td
 					{vertical-align:middle;text-align: center;}
.table-striped > tbody > tr:nth-of-type(odd){background:none}
.product{padding:20px 0;}
.order-nav .on{background:#009944}

.order-nav .m-list{padding:12px 0 0 38px;}
.order-nav .x-list{padding:12px  65px  12px 0px; text-align: right; }
.order-nav span{width:25%;text-align: right;background:#b5b5b5;height:5px;display: block;float: left;position:relative;z-index:1;}
.order-nav .span:after{content:"";display:block;height:25px;width:25px;border-radius:50%;border:5px solid #009944;position:absolute;z-index: 2;top:-10px;right:0;background:#FFFFFF}

.order-nav p{width:33.333334%;line-height:40px;padding-top:12px;}
/****************/
.table{border: 1px solid #A0A0A0;margin:0;}
.table > thead > tr > th{font-weight:normal;line-height:60px;font-size:18px;color:#000000;border-bottom:1px #A0A0A0 solid;background:#f3f3f3}
.table td a>img{border:1px solid #A0A0A0}
.table th:nth-last-child(3),.table th:nth-last-child(4){width:100px}
.table td p img{width:6%;float:right;margin:6px 80% 0 0;border:1px #A0A0A0 solid}
.table td p span{display: block;color:#009944}
a{color:#000000;cursor: pointer;}
.table td:nth-last-child(1)>a{width:100%;/*width:100px;*/}
.table td:nth-last-child(1) a:nth-child(2){color:#009944}
.table td:nth-last-child(2){/*width:150px;*/}
.table td:nth-last-child(2){color:#FF0000}
.table > tbody > tr > td{padding:20px 0;width:18%;}
.table td:nth-child(1){width:6%;}
.table td:nth-child(2){text-align: left;width:14%}
.table td:nth-child(3) {text-align: left;width:24%;}
.table td:nth-child(2) a{display: inline-block;}
.table td input[type="text"]{float:left;width:100px;border:0;line-height:18px;border-top:1px solid #A0A0A0;border-bottom:1px solid #A0A0A0;height:23px;text-align: center;}
.table td:nth-child(5){width:12.8%;}
.table td:nth-child(5) span{border:1px solid #A0A0A0;float:left;height:23px;width:23px;line-height:20px;display: inline-block;}

/*Order*/
.order-down{background:#79e6a9;line-height:67px;padding:0 35px 0 20px}
.order-down a{margin-left:20px;}
span i{color:#FF0000}
.order-down span b{color:#606060;}
.m-check{color:#000000 ;line-height:67px}
td .m-check span{border-color:#A0A0A0;}
td .m-check span{right:-10px;}
.m-check input:checked + span{border-color:#009944;}
.user-table>a{background:#009944;line-height:50px;padding:0 35px;display: block;font-size:20px;color:#FFFFFF}
