		 		var passw = function(v){
			
			var reg = /^([a-zA-Z0-9_\u4e00-\u9fa5]){6,16}$/
			if(reg.test(v)){
				 b.style.visibility="hidden";
			}else{
				 b.style.visibility="visible";
			}
		}
