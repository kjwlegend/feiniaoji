<!--pages/release/release.wxml-->
<!-- 头部选择部分 -->
<view class='header_back'>
  <view class='header_content'>
    <view class='place'>
      <view bindtap='choose_sp' class="{{start_place=='选择出发地'?'':'chasize'}}">{{start_place}}</view>
      <image src='/images/fly_cheng.png'></image>
      <view bindtap='choose_ep' class="{{end_place=='选择目的地'?'':'chasize'}}">{{end_place}}</view>
    </view>
    <Date id='dates' bind:getDate="getDate" count='true'></Date>
  </view>
</view>
<!-- 共享空间 -->
<view class='share'>
  <view class='icon'></view>
  <view class='can_share'>可共享空间<text class='zhuozhong'>*</text></view>
  <!-- <input placeholder-class='input_p' class="{{input?'input_on':'input_off'}}" value='24存行李箱'></input> -->
  <view class='add_input' wx:for='{{share}}' wx:key='{{index}}'>
    <input placeholder-class='input_p' class="{{index!=0?'input_on':'input_off'}}" placeholder='如:20寸行李箱' bindinput='share_item' data-index='{{index}}' value='{{item.value}}'></input>
    <image src='/images/quit.png' wx:if='{{index!=0}}' bindtap='reduce' data-index='{{index}}'></image>
  </view>
  <view class='add' bindtap='add_share'>
    <image src='/images/add_cheng.png'></image>
    <text class='xingli'>添加行李箱</text>
  </view>
</view>

<!-- 共享重量 -->
<view class='share_item'>
  <view class='icon'></view>
  <view class='can_share'>可共享重量<text class='zhuozhong'>*</text></view>
  <view class='share_kge'>
    <input type='number' placeholder-class='input_p' class="input_off" placeholder='请输入整数，如：6' bindinput='get_weight'></input>
    <text  class='xingli'>KG</text>
  </view>
</view>

<!-- 收获类型 -->
<view class='share_item' style='height:422rpx;'>
  <view class='icon'></view>
  <view class='can_share' style='margin-bottom:22rpx;'>收货类型<text class='zhuozhong'>*</text></view>
  <view class='goods_chose'>
    <view class="{{goods_on[index]==index?'goods_class_on':'goods_class_off'}}" wx:for="{{goods_class}}" wx:key="{{index}}" bindtap='choose_goods' data-num='{{index}}'>{{item}}</view>
  </view>
</view>

<!-- 备注信息 -->
<view class='share_item' style='margin-bottom:20rpx;'>
  <view class='icon'></view>
  <view class='can_share'>备注信息(选填)</view>
  <input style='width:612rpx;' placeholder-class='input_p' class="input_off input_beizhu" placeholder='在此输入备注信息' bindinput='get_info'></input>
</view>

<view>
  <label class='check' bindtap='agree'>
    <checkbox color='black' ></checkbox>
    已阅读<text style='color:#eea74c;' catchtap='xieyi_on'>《用户协议和隐私协议》</text>并同意其规则
  </label>
</view>

<button disabled='{{!can_sub}}' bindtap='go_release_success'>提  交</button>
